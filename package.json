{
  "private": true,
  "main": "./build/main-process.js",
  "scripts": {
    "build": "webpack --config=config/webpack.config.babel.js",
    "build:production": "NODE_ENV=production webpack --config=config/webpack.config.babel.js",
    "build:watch": "yarn build -- --watch",
    "build:package": "electron-packager . Wings --asar --overwrite",
    "lint": "eslint -c=config/eslintrc.json --ignore-path=.gitignore .",
    "start": "electron build/main-process.js"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.1",
    "bluebird": "^3.5.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "dynamic-css-properties": "^0.1.2",
    "electron": "1.6.10",
    "electron-packager": "^8.7.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.28.0",
    "js-yaml": "^3.8.4",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "stateful-react-container-webpack-plugin": "^0.2.1",
    "style-loader": "^0.18.1",
    "webpack": "^3.1.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8"
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ]
  }
}
